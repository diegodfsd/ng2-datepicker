    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-coverage-istanbul-reporter": "^0.2.0",
    "karma-jasmine": "^1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "^5.1.0",
    "rimraf": "^2.6.1",
    "rxjs": "^5.5.6",
    "ts-node": "^4.1.0",
    "tslint": "^5.9.1",
    "typescript": "~2.6.2",
    "zone.js": "^0.8.20"
  }
